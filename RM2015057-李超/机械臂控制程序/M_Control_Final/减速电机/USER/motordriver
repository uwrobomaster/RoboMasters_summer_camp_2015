{
  /* 配置系统时钟为72M */
	SystemInit();
 
	/* TIM3 PWM波输出初始化，并使能TIM3 PWM输出 */
	TIM3_PWM_Init();
//	delay_init(72);
  while (1)
 // delay_ms(1);
	//  P1=0X55;
	//  delay(i);
	//  P1=0Xaa;
	//  delay(i);
	
	

	
  {}
}


